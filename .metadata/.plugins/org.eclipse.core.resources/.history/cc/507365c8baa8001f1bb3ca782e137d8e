package defaultt;

public class Aluguel {

	public static void main(String[] args) {
		
		int dia = 8;
		int km = 720;
		int custoD = 60;
		double custokm = 0.15;
		int prod1 = (custoD * dia);
		double prod2 = (double) custokm * km;
		double total =(double) prod1 * prod2;
		System.out.println("the times of all is "+total);

	}

}
