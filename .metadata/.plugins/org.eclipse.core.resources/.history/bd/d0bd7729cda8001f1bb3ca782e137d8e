package defaultt;

public class Time {

	public static void main(String[] args) {
		int time = 4;
		if(time < 5) {
			System.out.println("happy midnight");
		}
		else if(time < 12) {
			System.out.println("Good morning");
		}
		else if(time < 18) {
			System.out.println("Good afternoon");
		}
		else if(time <= 23) {
			System.out.println("Good Evennig");
		}

	}

}
