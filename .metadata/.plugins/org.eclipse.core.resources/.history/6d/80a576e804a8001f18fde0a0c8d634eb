package defaultt;

public class Ifstatement {

	public static void main(String[] args) {
		int age = 27;
		int bird = 28;
		if(age < 18) {
			System.out.println("You can vote");
		}
		else if(age == 27 || bird == 29) {
			System.out.println("You can vote but fuck you need get a family");
		}

	}

}
