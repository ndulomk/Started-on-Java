package data;

public class Datatypes {

	public static void main(String[] args) {
		
		int x;//declaration
		
		x = 123; //assgnement
		byte x1 = 126;
		long mine = 1234455564444444444L;
		int y = 123;
		double fuk = 12.56;
		boolean nine = true;
		char symbol = '@';
		String name = "Eddgar";
		System.out.println("X times y is: " + x * y );
		System.out.println("Hello" + name);

	}

}
