package defaultt;

public class Aluguel {

	public static void main(String[] args) {
		
		int dia = 8;
		int km = 720;
		int custoD = 60;
		int custokm = 2;
		int prod1 = custoD * dia;
		int prod2 = custokm * km;
		int total = prod1 + prod2;
		System.out.println("the times of all is "+total);

	}

}
